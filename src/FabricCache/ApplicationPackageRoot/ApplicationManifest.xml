<?xml version="1.0" encoding="utf-8"?>
<ApplicationManifest ApplicationTypeName="FabricCacheAppType"
                     ApplicationTypeVersion="1.0.0"
                     xmlns="http://schemas.microsoft.com/2011/01/fabric"
                     xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Parameters>
    <Parameter Name="FabricCache_MinReplicaSetSize" DefaultValue="1" />
    <Parameter Name="FabricCache_TargetReplicaSetSize" DefaultValue="3" />
    <Parameter Name="FabricCache_PartitionCount" DefaultValue="1" />
  </Parameters>
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="FabricCacheSvcPkg" ServiceManifestVersion="1.0.0" />
    <ConfigOverrides />
  </ServiceManifestImport>
  <DefaultServices>
    <Service Name="FabricCache" ServicePackageActivationMode="ExclusiveProcess">
      <StatefulService ServiceTypeName="FabricCacheSvcType" TargetReplicaSetSize="[FabricCache_TargetReplicaSetSize]" MinReplicaSetSize="[FabricCache_MinReplicaSetSize]">
        <UniformInt64Partition PartitionCount="[FabricCache_PartitionCount]" LowKey="0" HighKey="4294967295" />
      </StatefulService>
    </Service>
  </DefaultServices>
</ApplicationManifest>